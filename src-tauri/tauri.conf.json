{
	"$schema": "./../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run build",
		"devPath": "./../index.html",
		"distDir": "./../dist/"
	},
	"package": {
		"productName": "tauri",
		"version": "0.0.1"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"window": {
				"setPosition": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "Copyright (c) 2022 Nikola Hristov. All rights reserved.",
			"externalBin": [],
			"icon": [
				"src/img/16x16.png",
				"src/img/32x32.png",
				"src/img/icon.ico"
			],
			"identifier": "ai.blackrainbow.tauri",
			"longDescription": "",
			"resources": ["../dist/*"],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": "",
				"wix": {
					"bannerPath": "src/img/banner.png",
					"dialogImagePath": "src/img/dialogImage.png"
				}
			}
		},
		"security": {
			"csp": null
		},
		"systemTray": {
			"iconAsTemplate": true,
			"iconPath": "src/img/icon.ico"
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"alwaysOnTop": true,
				"center": false,
				"decorations": true,
				"fileDropEnabled": false,
				"focus": true,
				"fullscreen": false,
				"height": 1080,
				"label": "fa",
				"maximized": false,
				"resizable": false,
				"skipTaskbar": false,
				"title": "Follow aircraft.",
				"transparent": true,
				"url": "https://flightradar24.com",
				"visible": true,
				"width": 1920,
				"x": 0,
				"y": 0
			},
			{
				"alwaysOnTop": true,
				"center": false,
				"decorations": true,
				"fileDropEnabled": false,
				"focus": true,
				"fullscreen": false,
				"height": 1080,
				"label": "ta",
				"maximized": false,
				"resizable": false,
				"skipTaskbar": false,
				"title": "Twitter automate.",
				"transparent": true,
				"url": "https://tweetdeck.twitter.com",
				"visible": true,
				"width": 1920,
				"x": 0,
				"y": 0
			}
		]
	}
}
